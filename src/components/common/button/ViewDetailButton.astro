---
interface Props {
  url: string;
}

const { url } = Astro.props;
---

<a href={url} class="view-details">
  View Details
  <svg
    xmlns="http://www.w3.org/2000/svg"
    width="16"
    height="16"
    viewBox="0 0 24 24"
    fill="none"
    stroke="currentColor"
    stroke-width="2"
  >
    <path d="M5 12h14M12 5l7 7-7 7"></path>
  </svg>
</a>
